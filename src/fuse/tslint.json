{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "mat",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "mat",
      "kebab-case"
    ],
    "component-class-suffix": false,
    "directive-class-suffix": false,
    "use-host-property-decorator": false,
    "member-ordering": false
  }
}
